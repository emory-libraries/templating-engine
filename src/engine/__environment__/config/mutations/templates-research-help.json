{
  "alias": {
    "system.title": "title"
  },
  "rename": {
    "subtitle": "introduction",
    "blocks": "content-block",
    "blocks.@.type": "blocks.@.block-type",
    "contacts": "links-list-contacts",
    "contacts.cards": "contacts.link-contact",
    "resources": "links-list-resources",
    "resource.cardss": "resources.link-resource"
  },
  "replace": {},
  "remove": [],
  "add": {},
  "evaluate": [
    {
      "target": "blocks.@.heading-level",
      "condition": "blocks.@.heading-level == 'Heading Level 2'",
      "value": 2
    },
    {
      "target": "blocks.@.heading-level",
      "condition": "blocks.@.heading-level == 'Heading Level 3'",
      "value": 3
    },
    {
      "target": "blocks.@.heading-level",
      "condition": "blocks.@.heading-level == 'Heading Level 4'",
      "value": 4
    },
    {
      "target": "blocks.@.heading-level",
      "condition": "blocks.@.heading-level == 'Heading Level 5'",
      "value": 5
    },
    {
      "target": "blocks.@.heading-level",
      "condition": "blocks.@.heading-level == 'Heading Level 6'",
      "value": 6
    }
  ],
  "repeatable": [
    "content-block",
    "links-list-contacts.link-contact",
    "links-list-resources.link-resource"
  ],
  "checkbox": [
    "add-contact",
    "add-resource-links",
    "link-list-resources.link-resource.@.link-customizations"
  ],
  "radio": [
    "content-block.@.block-type",
    "link-list-resources.link-resource.@.link-type"
  ],
  "timestamp": [
    "system.created.datetime",
    "system.modified.datetime",
    "system.published.datetime"
  ],
  "text": [
    "introduction",
    "content-block.@.heading-label",
    "content-block.@.image.caption",
    "content-block.@.image.photo-credit",
    "content-block.@.video-url",
    "link-list-resources.link-resource.@.link-label",
    "link-list-resources.link-resource.@.external-url"
  ],
  "html": [
    "content-block.@.wysiwyg"
  ]
}

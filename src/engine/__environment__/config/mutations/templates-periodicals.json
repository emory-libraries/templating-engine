{
  "alias": {
    "system.title": "title"
  },
  "rename": {
    "blocks": "content-block",
    "blocks.@.type": "blocks.@.block-type",
    "blocks.@.src": "blocks.@.image.image-file.path",
    "recent-issues": "links-list-recent-issues",
    "recent-issues.tiles": "recent-issues.link",
    "recent-issues.tiles.@.image.src": "recent-issues.tiles.@.image-file.path",
    "recent-issues.tiles.@.link.href": "recent-issues.tiles.@.cascade-file.path",
    "recent-issues.tiles.@.title": "recent-issues.tiles.@.link-label",
    "past-issues": "links-list-past-issues",
    "past-issues.items": "past-issues.link",
    "past-issues.items.@.link.href": "past-issues.items.@.cascade-file.path",
    "past-issues.items.@.title": "past-issues.items.@.link-label"
  },
  "replace": {},
  "remove": [],
  "add": {},
  "evaluate": [
    {
      "target": "blocks.@.heading-level",
      "condition": "blocks.@.heading-level == 'Heading Level 2'",
      "value": 2
    },
    {
      "target": "blocks.@.heading-level",
      "condition": "blocks.@.heading-level == 'Heading Level 3'",
      "value": 3
    },
    {
      "target": "blocks.@.heading-level",
      "condition": "blocks.@.heading-level == 'Heading Level 4'",
      "value": 4
    },
    {
      "target": "blocks.@.heading-level",
      "condition": "blocks.@.heading-level == 'Heading Level 5'",
      "value": 5
    },
    {
      "target": "blocks.@.heading-level",
      "condition": "blocks.@.heading-level == 'Heading Level 6'",
      "value": 6
    }
  ],
  "repeatable": [
    "content-block",
    "links-list-recent-issues.link",
    "links-list-past-issues.link"
  ],
  "checkbox": [],
  "radio": [
    "content-block.@.block-type"
  ],
  "timestamp": [],
  "text": [
    "content-block.@.heading-label",
    "content-block.@.image.caption",
    "content-block.@.image.photo-credit",
    "content-block.@.video-url",
    "links-list-recent-issues.heading",
    "links-list-recent-issues.link.@.link-label",
    "links-list-recent-issues.link.@.description",
    "links-list-past-issues.heading",
    "links-list-past-issues.link.@.link-label"
  ],
  "html": [
    "content-block.@.wysiwyg"
  ]
}
{
  "alias": {
    "system.title": "title"
  },
  "rename": {
    "subtitle": "introduction",
    "exhibits": "link",
    "exhibits.@.date": "exhibits.@.cascade-page.content.system-data-structure.dates",
    "exhibits.@.date.start": "exhibits.@.date.start-date",
    "exhibits.@.date.start.month": "exhibits.@.date.start.month-only",
    "exhibits.@.date.end": "exhibits.@.date.end-date",
    "exhibits.@.date.end.month": "exhibits.@.date.end.month-only",
    "exhibits.@.image.src": "exhibits.@.image.image-file.path",
    "exhibits.@.title": "exhibits.@.cascade-page.title"
  },
  "replace": {},
  "remove": [],
  "add": {},
  "evaluate": [
    {
      "target": "blocks.@.heading-level",
      "condition": "blocks.@.heading-level == 'Heading Level 2'",
      "value": 2
    },
    {
      "target": "blocks.@.heading-level",
      "condition": "blocks.@.heading-level == 'Heading Level 3'",
      "value": 3
    },
    {
      "target": "blocks.@.heading-level",
      "condition": "blocks.@.heading-level == 'Heading Level 4'",
      "value": 4
    },
    {
      "target": "blocks.@.heading-level",
      "condition": "blocks.@.heading-level == 'Heading Level 5'",
      "value": 5
    },
    {
      "target": "blocks.@.heading-level",
      "condition": "blocks.@.heading-level == 'Heading Level 6'",
      "value": 6
    }
  ],
  "repeatable": [
    "link"
  ],
  "checkbox": [
    "title-hero.hero-options",
    "link.@.link-customizations",
    "link.@.cascade-page.content.system-data-structure.dates.start-date.month-only",
    "link.@.cascade-page.content.system-data-structure.dates.end-date.month-only"
  ],
  "radio": [
    "link.@.link-type"
  ],
  "timestamp": [
    "system.created.datetime",
    "system.modified.datetime",
    "system.published.datetime"
  ],
  "text": [
    "introduction",
    "link.@.external-url",
    "link.@.link-label",
    "link.@.link-description",
    "link.@.image.image-file.caption"
  ],
  "html": []
}